{
    "name": "carpooling-server",
    "version": "0.1.0",
    "private": true,
    "main": "build/server.js",
    "type": "commonjs",
    "scripts": {
        "build": "babel src --out-dir build --copy-files --source-maps",
        "start-prod": "npm run build && cross-env NODE_ENV=production PORT=80 node build/server.js",
        "start-dev": "npm run build && cross-env NODE_ENV=development PORT=32203 node build/server.js",
        "test": "npm run build && cross-env NODE_ENV=development PORT=32203 mocha ./build/test --require @babel/register --require mocha-suppress-logs --recursive --exit",
        "clean": "rimraf node_modules build/*"
    },
    "devDependencies": {
        "@babel/cli": "^7.17.10",
        "@babel/core": "^7.17.10",
        "@babel/eslint-parser": "^7.17.0",
        "@babel/plugin-transform-runtime": "^7.17.10",
        "@babel/register": "^7.17.7",
        "@tsconfig/node16": "^1.0.2",
        "@tsconfig/node17": "^1.0.0",
        "@types/express": "^4.17.13",
        "cross-env": "^7.0.3",
        "eslint": "^8.14.0",
        "mocha": "^10.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@babel/node": "^7.17.10",
        "@babel/runtime": "^7.17.9",
        "@discordjs/rest": "^0.4.1",
        "cors": "^2.8.5",
        "discord-api-types": "^0.32.1",
        "discord.js": "^13.6.0",
        "express": "^4.18.1",
        "express-validator": "^6.14.0",
        "firebase-admin": "^10.1.0",
        "path": "^0.12.7"
    }
}
