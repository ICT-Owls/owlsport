{
    "name": "carpooling-server",
    "version": "0.1.0",
    "private": true,
    "main": "build/server.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start-prod": "npm run build && cross-env NODE_ENV=production PORT=80 node build/src/server.js",
        "start-dev": "npm run build && cross-env NODE_ENV=development PORT=32203 node build/src/server.js",
        "mocha-test": "mocha --recursive --exit",
        "clean": "rimraf node_modules build/*"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.17.0",
        "@tsconfig/node16": "^1.0.2",
        "@tsconfig/node17": "^1.0.0",
        "@types/express": "^4.17.13",
        "cross-env": "^7.0.3",
        "eslint": "^8.14.0",
        "mocha": "^10.0.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "@discordjs/rest": "^0.4.1",
        "cors": "^2.8.5",
        "discord-api-types": "^0.32.1",
        "discord.js": "^13.6.0",
        "express": "^4.18.1",
        "express-validator": "^6.14.0",
        "firebase-admin": "^10.1.0",
        "path": "^0.12.7"
    }
}
